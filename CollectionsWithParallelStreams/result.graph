#! /usr/bin/gnuplot -persist
set terminal jpeg size 640, 480
set output "result.jpg"
set logscale y
set logscale x 2
set xlabel "Streams count"
set ylabel "Array size"
set zlabel "Time (ms)" rotate by 90
set grid ytics lc rgb "#bbbbbb" lw 1 lt 0
set grid xtics lc rgb "#bbbbbb" lw 1 lt 0
set grid ztics lc rgb "#bbbbbb" lw 1 lt 0
set dgrid3d
set pm3d corners2color c2
splot "result.txt" using 2:1:($3/1e6) with pm3d notitle